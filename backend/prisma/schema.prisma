generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
	id			Int @id @default(autoincrement())
	createdAt	DateTime @default(now())
	updateAt	DateTime @updatedAt

	email	String @unique @db.VarChar(16)
	hash		String

	firstName	String?
	lastName	String?

	@@map("users")
}
